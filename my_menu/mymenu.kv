#:kivy 2.0.0

<MyWidget>:
    orientation: 'vertical'
    padding: 20
    spacing: 50

    Spinner:
        id: spinner_id
        text: 'press me'
        values: ['options one', 'options two', 'options three', 'options four']
        on_text:
            root.spinner_clicked(spinner_id.text)

    Label:
        id: click_label
        text: 'Hello World'
        font_size: 32